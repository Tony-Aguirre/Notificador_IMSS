        app:layout_constraintBottom_toTopOf="@+id/spnTez"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTitulo8" />
    <Spinner
        android:id="@+id/spnTez"
        android:layout_width="200dp"
