        app:layout_constraintTop_toBottomOf="@+id/txtTexto8" />
    <TextView
        android:id="@+id/txtTexto10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="10dp"
        android:text="@string/txtTexto10"
        android:textColor="@color/dark"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/txtTexto11"
        app:layout_constraintEnd_toStartOf="@+id/spnCompexion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTexto9" />
    <TextView
        android:id="@+id/txtTexto11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="10dp"
        android:text="@string/txtTexto11"
        android:textColor="@color/dark"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/txtTexto12"
        app:layout_constraintEnd_toStartOf="@+id/spnCabello"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTexto10" />
    <TextView
        android:id="@+id/txtTexto12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
