        app:layout_constraintBottom_toTopOf="@+id/spnCompexion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spnSexo" />
    <Spinner
        android:id="@+id/spnCompexion"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/spnCabello"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spnTez" />
    <Spinner
        android:id="@+id/spnCabello"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/spnColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spnCompexion" />
    <Spinner
        android:id="@+id/spnColor"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/spnOjos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spnCabello" />
    <Spinner
        android:id="@+id/spnOjos"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="5dp"
